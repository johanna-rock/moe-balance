{
  // Search type
  // Simulated annealing over swap moves to escape local minima.
  "search": "anneal", // simulated annealing swaps

  // Annealing parameters
  "anneal_iters": 200, // number of swap steps
  "anneal_t0": 1.0, // initial temperature
  "anneal_t1": 0.01, // final temperature

  // Number of randomized restarts
  "restarts": 2 // repeat search with different seeds
}
